                "webpack-dev-server": "^4.11.1",
                "yup": "^0.32.11"
        },
        "scripts": {
                "start": "react-scripts start",
                "build": "./node_modules/.bin/env-cmd -f ./.env cross-env GENERATE_SOURCEMAP=false react-scripts build",
                "test": "./node_modules/.bin/react-scripts test",
                "coverage:dev": "react-scripts test --coverage --watchAll=false --testTimeout=50000"
        },
        "eslintConfig": {
                "extends": [
                        "react-app",
                        "react-app/jest"
                ]
        },
        "browserslist": {
                "production": [
                        ">0.2%",
                        "not dead",
                        "not op_mini all"
                ],
                "development": [
                        "last 1 chrome version",
                        "last 1 firefox version",
                        "last 1 safari version"
                ]
        },
        "jest": {
                "coveragePathIgnorePatterns": [
                        "src/interfaces",
                        "src/utils",
                        "src/redux",
                        "src/constants"
                ]
        },
        "jestSonar": {
                "reportPath": "reports",
                "reportFile": "test-report.xml",
                "indent": 4
        },
        "devDependencies": {
                "@types/reach__router": "^1.3.11",
                "@types/styled-components": "^5.1.26"
        }
}
