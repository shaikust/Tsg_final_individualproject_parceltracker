                "@types/react-router-dom": "^5.3.3",
                "@typescript-eslint/eslint-plugin": "^5.48.2",
                "@typescript-eslint/parser": "^5.48.2",
                "axios": "^1.2.3",
                "env-cmd": "^10.1.0",
                "eslint": "^8.32.0",
                "eslint-plugin-react": "^7.32.1",
                "formik": "^2.2.9",
                "husky": "^8.0.3",
                "jest-sonar-reporter": "^2.0.0",
                "jwt-decode": "^3.1.2",
                "lint-staged": "^13.1.0",
                "prettier": "^2.8.3",
                "react": "^18.2.0",
                "react-bootstrap": "^2.7.0",
                "react-dom": "^18.2.0",
                "react-redux": "^8.0.5",
